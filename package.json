{
  "name": "ksp-school",
  "scripts": {
    "dev": "npx @tailwindcss/cli -i ./css/tailwind.css -o ./school/static/app.css --watch",
    "build": "npx @tailwindcss/cli -i ./css/tailwind.css -o ./school/static/app.css --minify",
    "build-js": "esbuild school/bundle.js --bundle --minify --outfile=./school/static/bundle.js"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.10",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "alpinejs": "^3.14.9",
    "esbuild": "^0.25.5",
    "feather-icons": "^4.29.2",
    "htmx.org": "^1.9.12",
    "iconify-icon": "^1.0.8",
    "tailwindcss": "^4.1.10",
    "tippy.js": "^6.3.7"
  }
}
